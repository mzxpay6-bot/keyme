{
  "api": {
    "port": 3000,
    "cors": {
      "origin": "*",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "allowedHeaders": ["Content-Type", "Authorization"]
    },
    "rateLimit": {
      "windowMs": 60000,
      "max": 100
    }
  },
  "roblox": {
    "delayBetweenAccounts": 2500,
    "timeout": 10000,
    "maxRetries": 3
  },
  "security": {
    "requireApiKey": false,
    "apiKey": null
  }
}